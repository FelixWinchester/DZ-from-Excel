#include <iostream>
using namespace std;
int main() {
	int n, m, k1, k2, s = 0;
	cin >> n >> m;
	cin >> k1 >> k2;
	int** a = new int* [n];
	for (int i = 0; i < n; i++) {
		a[i] = new int[m];
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			cin >> a[i][j];
		}
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (j >= k1 && j <= k2) {
				s += a[i][j];

			}
		}
	}
	cout << s;
}
