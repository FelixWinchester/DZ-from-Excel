#include <iostream>
using namespace std;
int main() {
	int n, c, x;
	cin >> n;
	int maxi = -1000000;
	int mini = 100000;
	int* a = new int[n];
	for (int i = 0; i < n; i++) {
		int k;
		cin >> k;
		a[i] = k;
		if (k > maxi) {
			maxi = k;
			c = i;
		}

	}
	x = a[0];
	a[0] = maxi;
	a[c] = x;
	for (int i = 0; i < n; i++) {
		cout << a[i] << " ";
	}
}
	
